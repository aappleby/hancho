  "version": "0.2.0",
  "configurations": [
    {
      "name": "Scratch",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/hancho.py",
      "cwd": "${workspaceFolder}/scratch",
      //"args": ["--force", "-j1"],
      "console": "integratedTerminal",
      "justMyCode": false,
      "preLaunchTask": "Wipe build",
    },
    {
      "name": "Debug hancho.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/hancho.py",
      "cwd": "${workspaceFolder}",
      //"args": ["-f", "examples/gb_spu/gb_spu.hancho"],
      "console": "integratedTerminal",
      "justMyCode": false,
      "preLaunchTask": "Wipe build",
    },
    {
      "name": "Debug test.hancho",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/hancho.py",
      "cwd": "${workspaceFolder}",
      "args": ["-f", "test.hancho"],
      "console": "integratedTerminal",
      "justMyCode": false,
      "preLaunchTask": "Wipe build",
    },
    {
      "name": "Debug run_tests.py",
      "type": "debugpy",
      "request": "launch",
      "program": "run_tests.py",
      "cwd": "${workspaceFolder}/tests",
      //"args": ["TestHancho.test_raw_task"],
      "console": "integratedTerminal",
      "justMyCode": false,
    },
    {
      "name": "Debug Megabuild",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/hancho.py",
      "cwd": "${workspaceFolder}/..",
      //"args": ["--force", "-j1"],
      "console": "integratedTerminal",
      "justMyCode": false,
      "preLaunchTask": "Wipe build",
    },

    {
      "name": "Debug Scratch",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scratch.py",
      "cwd": "${workspaceFolder}/..",
      "console": "integratedTerminal",
      "justMyCode": false,
    },

  ]
}
